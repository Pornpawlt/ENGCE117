#include <stdio.h>

void GetSet(int a[], int *n);

int main() {
    int data[50];
    int num;
    GetSet(data, &num);
    printf("\nจำนวนสมาชิก = %d\n", num);
    printf("ข้อมูลที่รับมา : ");
    for(int i=0; i<num; i++){
        printf("%d ", data[i]);
    }return 0;
}
void GetSet(int a[], int *n) {
    int i;
    printf("ใส่จำนวนสมาชิก: ");
    scanf("%d", n);
    for(i=0; i<*n; i++){
        printf("สมาชิกตัวที่ %d : ", i+1);
        scanf("%d", &a[i]);
    }
}
