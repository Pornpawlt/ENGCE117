#include <stdio.h>

int Getset(int data[]) ;

int main() {
    int data[1000] ;
    int num;

    num = Getset(data) ;

    printf("Numbers collected:\n") ;
    for (int i = 0; i < num; i++) {
        printf("%d ", data[i]) ;
    }
    printf("\n") ;

    return 0;
}

int Getset(int data[]) {
    int N ;

    printf("Enter Data Size: ") ;
    scanf("%d", &N) ;
    
    for (int i = 0; i < N; i++) {
        printf("Enter %d integers : ", N) ;
        scanf("%d", &data[i]) ;
    }

    return N ;
}
