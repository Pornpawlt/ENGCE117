#include <stdio.h>

void GetSet(int data[], int *n);

int main() {
    int data[100], num;

    GetSet(data, &num);

    printf("\nจำนวนสมาชิกในเซต = %d\n", num);
    printf("สมาชิกในเซตคือ: ");
    for(int i = 0; i < num; i++) {
        printf("%d ", data[i]);
    }
    return 0;
}

void GetSet(int data[], int *n) {
    int i;

    printf("ป้อนจำนวนสมาชิกในเซต: ");
    scanf("%d", n);

    for(i = 0; i < *n; i++) {
        printf("ป้อนสมาชิกตัวที่ %d: ", i + 1);
        scanf("%d", &data[i]);
    }
}
