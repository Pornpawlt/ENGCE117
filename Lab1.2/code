#include <stdio.h>

void   GetSet ( int x[] , int*n ) ;

int main( ){
 int d[100] , nn  ;
   
    GetSet( d ,  &nn );

 printf("num = %d\n" ,nn );
   printf("data : ");
 for(int  i =0 ;i< nn ;i++ ){
  printf("%d ",d[i] );
 }
 return 0 ;
}

void   GetSet ( int x[] , int*n ) 
{
 int i ;
    printf("ใส่จำนวน: ");
   scanf("%d",n );

for( i=0 ; i< *n ; i++ ){
  printf("ตัว %d --> ",i+1 );
 scanf("%d",&x [ i ] );
}

}
