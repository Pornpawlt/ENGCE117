#include <stdio.h>

void GetSet ( int [] , int *num ) ;
int main() {
    int *data , num ;
    GetSet( data,  &num ) ;
    return 0 ;
}
void GetSet ( int data[] , int *num) {
    printf("data : ") ;
    scanf("%d", num) ;
    for( int i = 0; i < *num ; i++){
        printf("pleas enter number :") ;
        scanf("%d", &data[i]) ;
    }
    for( int i = 0; i < *num ; i++){
        printf("[%d]", data[i]) ;
    }
}
